{
   "compilerOptions":
   {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "Node",
      "baseUrl": "./",
      "paths": {
         "@/*": ["src/*"],
         "Assets/*": ["src/assets/*"],
         "Components/*": ["src/components/*"],
         "Core/*": ["src/core/*"],
         "Utilities/*": ["src/utilities/*"]
      }
   },
   "include": [
      "./src/*"
   ],
   "exclude": [
      "./node_modules/*",
      "./bundler/*"
   ]
}
